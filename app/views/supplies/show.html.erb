<%- model_class = Supply -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:data) %>:</strong></dt>
  <dd><%= @supply.data %></dd>
  <dt><strong><%= model_class.human_attribute_name(:posto) %>:</strong></dt>
  <dd><%= @supply.posto %></dd>
  <dt><strong><%= model_class.human_attribute_name(:quilometragem) %>:</strong></dt>
  <dd><%= @supply.quilometragem %></dd>
  <dt><strong><%= model_class.human_attribute_name(:litros) %>:</strong></dt>
  <dd><%= @supply.litros %></dd>
  <dt><strong><%= model_class.human_attribute_name(:combustivel) %>:</strong></dt>
  <dd><%= @supply.combustivel %></dd>
  <dt><strong><%= model_class.human_attribute_name(:preco) %>:</strong></dt>
  <dd><%= @supply.preco %></dd>
  <dt><strong><%= model_class.human_attribute_name(:user_id) %>:</strong></dt>
  <dd><%= @supply.user_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:car_id) %>:</strong></dt>
  <dd><%= @supply.car_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              supplies_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_supply_path(@supply), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              supply_path(@supply),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
